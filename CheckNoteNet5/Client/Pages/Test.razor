@page "/test"
@using CheckNoteNet5.Shared.Services
@inject INoteService NoteService

@text

@code
{
    private string text;

    protected override async Task OnInitializedAsync()
    {
        var result = await NoteService.Get(2);

        if (result.IsOk)
        {
            text = result.Value.Title;
        }
    }
}
